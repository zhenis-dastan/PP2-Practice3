2
class Dog:
    def bark(self):
        print("Woof!")

dog = Dog()
dog.bark()



3
class Car:
    def __init__(self, brand):
        self.brand = brand

car = Car("Toyota")
print(car.brand)



4
class Calculator:
    def add(self, a, b):
        return a + b

calc = Calculator()
print(calc.add(5, 6))



5
class Student:
    school = "ABC School"

print(Student.school)


